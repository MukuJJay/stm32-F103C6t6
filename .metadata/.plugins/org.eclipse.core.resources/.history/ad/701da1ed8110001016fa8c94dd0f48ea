#include<stdio.h>
#include<stdint.h>

int main(void){
	__asm volatile("SVC #8");

	for(;;);
}

__attribute__ ((naked)) void SVC_Handler(void){
	__asm("MRS R0, MSP");
	__asm("B SVC_Handler_c");
}

void SVC_Handler_c (uint32_t* pBaseStackFrame){
	printf("hokage");

//	uint8_t* pReturnAddress = (uint8_t*) pBaseStackFrame[6];
}
